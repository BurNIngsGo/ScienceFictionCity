<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <servlet>
        <servlet-name>Tests</servlet-name>
        <servlet-class>com.sfc.servlet.Tests</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>Tests</servlet-name>
        <url-pattern>/Tests</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.sfc.servlet.LoginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PaiHang</servlet-name>
        <servlet-class>com.sfc.servlet.PaiHangServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>PaiHang</servlet-name>
        <url-pattern>/PaiHang</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>S2pr/index.html</welcome-file>
        <welcome-file>webPage/index1.html</welcome-file>
    </welcome-file-list>
</web-app>